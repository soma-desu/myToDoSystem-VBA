{
    "description": "機能やIOを定義します",
    "methodBundle": {
        "basicManagementOfToDo": {
            "description": "ToDoの基本管理機能を提供します",
            "methods": [
                {
                    "addBasicToDo": "新規BasicToDoの追加"
                },
                {
                    "editBasicToDo": "既存BasicToDoの編集"
                },
                {
                    "doneBasicToDo": "既存BasicToDoの完了"
                },
                {
                    "duplicateBasicToDo": "既存BasicToDoの複製"
                },
                {
                    "deleteBasicToDo": "既存BasicToDoの削除"
                },
                {
                    "addRepeatToDo": "新規RepeatToDoの追加"
                },
                {
                    "editRepeatToDo": "既存RepeatToDoの編集"
                },
                {
                    "doneRepeatToDo": "既存RepeatToDoの完了"
                },
                {
                    "duplicateRepeatToDo": "既存RepeatToDoの複製"
                },
                {
                    "deleteRepeatToDo": "既存RepeatToDoの削除"
                }
            ]
        },
        "displayTodayToDoList": {
            "description": "今日のToDo一覧を表示します",
            "methods": [
                {
                    "displayTodayToDoList": "今日のToDo一覧を表示します"
                }
            ]
        },
        "displayTomorrowToDoList": {
            "description": "明日のToDo一覧を表示します",
            "methods": [
                {
                    "displayTomorrowToDoList": "明日のToDo一覧を表示します"
                }
            ]
        },
        "displaySpecificDayToDoList": {
            "description": "特定の日のToDo一覧を表示します",
            "methods": [
                {
                    "displaySpecificDayToDoList": "特定の日のToDo一覧を表示します"
                }
            ]
        },
        "displayTwoWeekToDoScheduleCalendar": {
            "description": "2週間分のToDo実行予定カレンダーを表示します",
            "methods": [
                {
                    "displayTwoWeekToDoScheduleCalendar": "2週間分のToDo実行予定カレンダーを表示します"
                }
            ]
        },
        "displayDailyExecutedTaskTimeOfMilestone": {
            "description": "1か月分の日別実行済みタスクの実所要時間のMilestone別集計と表示します",
            "methods": [
                {
                    "displayDailyExecutedTaskTimeOfMilestone": "1か月分の日別実行済みタスクの実所要時間のMilestone別集計と表示します"
                }
            ]
        }
    },
    "ioBundle": {
        "inputPlaces":{
            "executedTaskConfirmationSheet": {
                "criteriaTable": {
                    "queryUpdateCell": {
                        "description": "クエリ更新",
                        "acceptEvent": ["doubleClick"],
                        "acceptMethod": ["displayDailyExecutedTaskTimeOfMilestone"]
                    }
                }
            },
            "twoWeekScheduleConfirmationSheet": {
                "criteriaTable": {
                    "queryUpdateCell": {
                        "description": "クエリ更新",
                        "acceptEvent": ["doubleClick"],
                        "acceptMethod": ["displayTwoWeekToDoScheduleCalendar"]
                    }
                }
            },
            "todayToDoSheet": {
                "criteriaTable": {
                    "queryUpdateCell": {
                        "description": "クエリ更新",
                        "acceptEvent": ["doubleClick"],
                        "acceptMethod": ["displayTodayToDoList"]
                    },
                    "addNewToDoCell": {
                        "description": "新規ToDo追加",
                        "acceptEvent": ["doubleClick"],
                        "acceptMethod": ["addBasicToDo", "addRepeatToDo"]
                    }
                },
                "toDoTable": {
                    "campaignNameColumn": {
                        "description": "campaignName列",
                        "acceptEvent": ["rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo"]
                    },
                    "milestoneNameColumn": {
                        "description": "milestoneName列",
                        "acceptEvent": ["rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo"]
                    },
                    "toDoNameColumn": {
                        "description": "toDoName列",
                        "acceptEvent": ["rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo"]
                    },
                    "toDoTypeNameColumn": {
                        "description": "toDoTypeName列",
                        "acceptEvent": ["rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo"]
                    },
                    "scheduledExecutionTimeColumn": {
                        "description": "scheduledExecutionTime列",
                        "acceptEvent": ["rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo"]
                    },
                    "estimatedExecutionalDurationColumn": {
                        "description": "estimatedExecutionalDuration列",
                        "acceptEvent": ["rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo"]
                    },
                    "scheduledResttimeDurationColumn": {
                        "description": "scheduledResttimeDuration列",
                        "acceptEvent": ["rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo"]
                    },
                    "deadlineDateAndTimeColumn": {
                        "description": "deadlineDateAndTime列",
                        "acceptEvent": ["rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo"]
                    },
                    "actualBeginningTimeColumn": {
                        "description": "actualBeginningTime列",
                        "acceptEvent": ["doubleClick", "rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo"]
                    },
                    "actualFinishTimeColumn": {
                        "description": "actualFinishTime列",
                        "acceptEvent": ["doubleClick", "rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo", "doneBasicToDo", "doneRepeatToDo"]
                    },
                    "priorityColumn": {
                        "description": "priority列",
                        "acceptEvent": ["rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo"]
                    },
                    "scheduledExecutionDateColumn": {
                        "description": "scheduledExecutionDate列",
                        "acceptEvent": ["rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo"]
                    },
                    "editColumn": {
                        "description": "edit列",
                        "acceptEvent": ["doubleClick", "rewrite"],
                        "acceptMethod": ["editBasicToDo", "editRepeatToDo"]
                    },
                    "duplicateColumn": {
                        "description": "duplicate列",
                        "acceptEvent": ["doubleClick", "rewrite"],
                        "acceptMethod": ["duplicateBasicToDo", "duplicateRepeatToDo"]
                    },
                    "deleteColumn": {
                        "description": "delete列",
                        "acceptEvent": ["doubleClick", "rewrite"],
                        "acceptMethod": ["deleteBasicToDo", "deleteRepeatToDo"]
                    }
                }
            },
            "nextWorkDayToDoSheet": { 
                "sameAs": "todayToDoSheet" 
            },
            "specificDayToDoSheet": { 
                "sameAs": "todayToDoSheet" 
            }
        },
